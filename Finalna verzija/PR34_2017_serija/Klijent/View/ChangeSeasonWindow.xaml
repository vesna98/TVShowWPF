<Window x:Class="Klijent.View.ChangeSeasonWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Klijent.View"
        mc:Ignorable="d"
        Title="ChangeSeasonWindow" Height="300" Width="400" Background="#2e3137">
    <Grid>
        <Button Content="Change" Command="{Binding ChangeSeasonCommand}" Background="#4CAF50" Foreground="White" HorizontalAlignment="Left" Height="29" Margin="85,131,0,0" VerticalAlignment="Top" Width="234"/>
        <Label Content="Number" Foreground="White" HorizontalAlignment="Left" Height="28" Margin="25,72,0,0" VerticalAlignment="Top" Width="55"/>
        <TextBox HorizontalAlignment="Left" Height="28" Margin="85,72,0,0" Text="{Binding ChangeS.Number, UpdateSourceTrigger=PropertyChanged}" TextWrapping="Wrap"  VerticalAlignment="Top" Width="234"/>
        <Label x:Name="label" Foreground="White" Content="Izmenite sezonu" FontSize="20" HorizontalAlignment="Left" Margin="25,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
